{
    "list.previewHighlightGroup": "Statement",
    "list.nextKeymap": "<C-l>",
    "list.previousKeymap": "<C-j>",
    "suggest.noselect": true,
    "suggest.enablePreselect": false,
    "explorer.file.showHiddenFiles": true,
    "explorer.keyMappingMode": "none",
    "explorer.keyMapping.global": {
    },
    "languageserver": {
		"lua": {
			"command": "lua-lsp",
			"filetypes": ["lua"]
		},
		"swift": {
			"command": "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/bin/sourcekit-lsp",
			"args": [],
			"filetypes": ["swift"],
			"initializationOptions": {},
			"settings": {}
		},
		"golang": {
			"command": "gopls",
			"rootPatterns": ["go.mod"],
			"filetypes": ["go"],
			"initializationOptions": {
				"gocodeCompletionEnabled": true,
				"diagnosticsEnabled": true,
				"lintTool": "golint"
			}
		},
		"bash": {
			"command": "bash-language-server",
			"args": ["start"],
			"filetypes": ["sh"],
			"ignoredRootPaths": []
		},
		"racket": {
			"command": "racket",
			"args": ["--lib", "racket-langserver"],
			"filetypes": ["scheme", "racket"]
		},
		"terraform": {
			"command": "terraform-ls",
			"args": ["serve"],
			"filetypes": ["terraform", "tf"],
			"initializationOptions": {},
			"settings": {}
		},
		"ccls": {
			"command": "ccls",
			"filetypes": ["c", "cc", "cpp", "c++", "objc", "objcpp"],
			"rootPatterns": [".ccls", ".ccls-root", "compile_commands.json", ".git/", ".hg/"],
			"initializationOptions": {
				"cache": {
					"directory": "/tmp/ccls"
				}
			}
		}
	}
 }
